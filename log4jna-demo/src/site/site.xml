<project name="Log4JNA" xmlns="http://maven.apache.org/DECORATION/1.6.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd">
	<version position="right" />
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.4</version>
	</skin>
	<custom>
		<fluidoSkin>
			<topBarEnabled>false</topBarEnabled>
			<sideBarEnabled>true</sideBarEnabled>
			<googleSearch>false</googleSearch>
			<leftColumnClass>span2</leftColumnClass>
			<bodyColumnClass>span10</bodyColumnClass>
		</fluidoSkin>
	</custom>
	<bannerLeft>
		<name>${project.name}</name>
		<title></title>
		<src>../parent/img/log4jna.jpg</src>
		<alt>log4jna</alt>
	</bannerLeft>
	<bannerRight>
		<name>${project.name}</name>
		<alt>${project.name}</alt>
		<href>/index.html</href>
	</bannerRight>
	<body>
		<links>
			<item name="${project.name}" href="https://github.com/dblock/log4jna/" />
		</links>
		<menu ref="reports"></menu>
	</body>
</project>
