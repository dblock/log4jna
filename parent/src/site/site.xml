    <menu name="Log4JNA">
      <item name="Home" href="./index.html" />
    </menu>
    <menu name="Guides and Documentation">
      <item name="Usage" href="/guide/index.html" collapse="false">
        <item name="How to use the BOM" href="/guide/usage.html" />
        <item name="How to maintain the BOM" href="/guide/develop.html" />
